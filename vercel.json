{
  "version": 2,
  "functions": {
    "api/mass-mail/index.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    },
    "api/mass-mail/auth/google.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    },
    "api/mass-mail/auth/google/callback.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/mass-mail",
      "destination": "/api/mass-mail/index.js"
    },
    {
      "source": "/api/mass-mail/auth/google",
      "destination": "/api/mass-mail/auth/google.js"
    },
    {
      "source": "/api/mass-mail/auth/google/callback",
      "destination": "/api/mass-mail/auth/google/callback.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}