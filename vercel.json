{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/certificates/(.*)",
      "dest": "/api/certificates/index.js"
    },
    {
      "src": "/api/reports/(.*)",
      "dest": "/api/reports/index.js"
    },
    {
      "src": "/api/emails/(.*)",
      "dest": "/api/emails/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}