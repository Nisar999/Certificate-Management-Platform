{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/certificates/(.*)",
      "dest": "/api/certificates/index.js"
    },
    {
      "src": "/api/reports/(.*)",
      "dest": "/api/reports/index.js"
    },
    {
      "src": "/api/emails/(.*)",
      "dest": "/api/emails/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}