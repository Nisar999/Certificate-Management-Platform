{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install"
  "rewrites": [
    {
      "source": "/api/mass-mail/auth/google/callback",
      "destination": "/api/mass-mail"
    },
    {
      "source": "/api/mass-mail/auth/google",
      "destination": "/api/mass-mail"
    },
    {
      "source": "/api/mass-mail/(.*)",
      "destination": "/api/mass-mail"
    }
  ]
}
